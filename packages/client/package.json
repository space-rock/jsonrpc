{
  "name": "@space-rock/jsonrpc-client",
  "version": "0.2.7",
  "description": "JSON RPC fetch client for NEAR Protocol",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm clean && pnpm rollup -c",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "near",
    "jsonrpc",
    "client",
    "fetch"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/@near-js/jsonrpc-client.git",
    "directory": "packages/client"
  },
  "homepage": "https://github.com/@near-js/jsonrpc-client#readme",
  "bugs": {
    "url": "https://github.com/@near-js/jsonrpc-client/issues"
  },
  "dependencies": {
    "@space-rock/jsonrpc-types": "workspace:^",
    "zod": "~4.0.10"
  },
  "devDependencies": {
    "@types/node": "~22.16.4",
    "typescript": "~5.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
