{
  "name": "@space-rock/jsonrpc-codegen",
  "version": "0.1.0",
  "description": "Code generation scripts for NEAR Protocol JSON RPC types",
  "private": true,
  "type": "module",
  "scripts": {
    "generate": "pnpm generate:types && pnpm generate:schemas && pnpm generate:tests && pnpm validate",
    "generate:types": "tsx src/types.ts",
    "generate:schemas": "tsx src/schemas.ts",
    "generate:schemas-valibot": "tsx src/schemas-valibot.ts",
    "generate:tests": "tsx src/tests.ts",
    "validate": "tsx src/validate.ts"
  },
  "keywords": [
    "near",
    "jsonrpc",
    "codegen",
    "types"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "~22.16.4",
    "chalk": "^5.4.1",
    "openapi-typescript": "7.8.0",
    "ts-morph": "26.0.0",
    "ts-to-zod": "3.15.0",
    "tsx": "~4.20.3",
    "typescript": "~5.8.3"
  }
}
