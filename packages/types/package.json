{
  "name": "@space-rock/jsonrpc-types",
  "version": "0.3.2",
  "description": "TypeScript types for NEAR Protocol JSON RPC client",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "near",
    "jsonrpc",
    "types",
    "typescript"
  ],
  "author": "NEAR Inc",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/near/jsonrpc-client-js.git",
    "directory": "packages/types"
  },
  "homepage": "https://github.com/near/jsonrpc-client-js#readme",
  "bugs": {
    "url": "https://github.com/near/jsonrpc-client-js/issues"
  },
  "dependencies": {
    "valibot": "~1.1.0"
  },
  "devDependencies": {
    "typescript": "~5.8.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
